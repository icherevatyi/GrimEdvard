}

[application]

config/name="Grim Edvard"
run/main_scene="res://src/components/Levels/Forest/Forest.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://src/components/Global/Global.gd"
LevelTransition="*res://src/components/Global/Helpers/LevelTransition.gd"
